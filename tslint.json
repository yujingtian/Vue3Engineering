{
    "defaultSeverity": "warning",
    "extends": [
      "tslint:recommended"
    ],
    "linterOptions": {
      "exclude": [
        "node_modules/**"
      ]
    },
    "no-shadowed-variable": [
      true,
      {
        "class": true,
        "enum": true,
        "function": true,
        "interface": false,
        "namespace": true,
        "typeAlias": false,
        "typeParameter": false
      }
    ],
    "rules": {
      "no-console": false,
      "indent": [true, "spaces", 2],
      "interface-name": false,
      "no-consecutive-blank-lines": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "no-shadowed-variable": [
        true,
        {
          "class": false,
          "enum": true,
          "function": true,
          "interface": true,
          "namespace": true,
          "typeAlias": false,
          "typeParameter": false
        }
      ]
    }
  }
  